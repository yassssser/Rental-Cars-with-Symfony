{% extends 'back.html.twig' %}

{% block content %}
    <h1>Agence creation</h1>


    {{ form_start(form) }}
    <div class="row">
        <div class="col-md-6 mx-auto">
            <div class="form-group">
                {{ form_row(form.libelle, {'attr': {'class': 'form-control'}}) }}
            </div>
            <div class="form-group">
                {{ form_row(form.adresse, {'attr': {'class': 'form-control'}}) }}
            </div>
        </div>
    </div>
    <div class="row mt-2">
        <div class="col-md-6 mx-auto">
            <input class="btn btn-success btn-block" type="submit" value="Create" />
        </div>
    </div>
    <a  class="btn btn-primary" href="{{ path('admin_agence_index') }}"><i class="fa fa-arrow-left"></i>  Back to the list</a>
     
    {{ form_end(form) }}
{% endblock %}

{% block breadcrumb %}
    <ul class="breadcrumb">
        <li><a href=""> Dashboard /  </a></li>
        <li class="ml-2"><a href=""> Liste des agneces / </a></li>
        <li class="active"> Nouvelle Agence</li>
    </ul>

{% endblock %}
